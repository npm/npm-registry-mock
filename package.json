{
  "name": "npm-registry-mock",
  "version": "1.3.2",
  "description": "mock the npm registry",
  "main": "index.js",
  "scripts": {
    "test": "tap",
    "lint": "eslint \"**/*.js\"",
    "postlint": "template-oss-check",
